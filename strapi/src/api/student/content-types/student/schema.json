{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "required": true,
      "regex": "/^(\\p{L}+[ -'])*\\p{L}+$/u"
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "required": false,
      "regex": "/^(\\p{L}+[ -'])*\\p{L}+$/u"
    },
    "email": {
      "type": "email",
      "unique": true
    },
    "password": {
      "type": "password"
    },
    "joinedAt": {
      "type": "date",
      "required": true
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    }
  }
}
