{
  "kind": "collectionType",
  "collectionName": "content_creators",
  "info": {
    "singularName": "content-creator",
    "pluralName": "content-creators",
    "displayName": "Content Creator"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "required": true
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "required": true
    },
    "verifiedAt": {
      "type": "date"
    },
    "motivation": {
      "type": "text",
      "maxLength": 400,
      "required": false
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": false,
      "minLength": 8
    },
    "currentCompany": {
      "type": "string",
      "minLength": 1,
      "maxLength": 40,
      "required": false
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "content_creators"
    },
    "user_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-log.user-log"
    },
    "dashboard_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dashboard-activity.dashboard-activity",
      "mappedBy": "content_creator"
    }
  }
}
