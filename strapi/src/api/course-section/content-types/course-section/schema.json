{
  "kind": "collectionType",
  "collectionName": "course_sections",
  "info": {
    "singularName": "course-section",
    "pluralName": "course-sections",
    "displayName": "Course Section"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "required": true
    },
    "Description": {
      "type": "text",
      "maxLength": 400
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "course_sections"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity.activity"
    }
  }
}
