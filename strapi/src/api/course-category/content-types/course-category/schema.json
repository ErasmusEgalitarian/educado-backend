{
  "kind": "collectionType",
  "collectionName": "course_categories",
  "info": {
    "singularName": "course-category",
    "pluralName": "course-categories",
    "displayName": "Course Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "required": true,
      "unique": true
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "course_categories"
    }
  }
}
