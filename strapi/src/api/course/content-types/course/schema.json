{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback"
    },
    "content_creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::content-creator.content-creator",
      "inversedBy": "courses"
    },
    "course_selections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-selection.course-selection"
    },
    "difficulty": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 3
    },
    "statusEnum": {
      "type": "enumeration",
      "required": true,
      "default": "draft",
      "enum": [
        "draft",
        "published",
        "hidden"
      ]
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "personal finance",
        "health and workplace safety",
        "sewing",
        "electronics"
      ]
    },
    "numOfRatings": {
      "type": "integer",
      "default": 0
    },
    "numOfSubscriptions": {
      "type": "integer",
      "default": 0
    }
  }
}
